    <servlet-mapping>
        <servlet-name>servlet</servlet-name>
        <url-pattern>/Controller/galleryShow.go</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet</servlet-name>
        <url-pattern>/Controller/admin.go</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet</servlet-name>
        <url-pattern>/Controller/getImge2.go</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet</servlet-name>
        <url-pattern>/Controller/ditle.go</url-pattern>
